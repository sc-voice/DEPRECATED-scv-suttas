    {
      "segments": [
        {
          "scid": "sn37.10:1.1",
          "pli": "“Pañcahi, anuruddha, dhammehi samannāgato mātugāmo … pe … nirayaṃ upapajjati.",
          "en": ""
        },
        {
          "scid": "sn37.10:1.2",
          "pli": "Katamehi pañcahi?",
          "en": ""
        },
        {
          "scid": "sn37.10:1.3",
          "pli": "Assaddho ca hoti, ahiriko ca hoti, anottappī ca hoti, dussīlo ca hoti, duppañño ca hoti—",
          "en": "“… They’re faithless, shameless, imprudent, unethical, and witless. …”"
        },
        {
          "scid": "sn37.10:1.4",
          "pli": "imehi kho, anuruddha, pañcahi dhammehi samannāgato mātugāmo … pe … nirayaṃ upapajjatī”ti.",
          "en": ""
        },
        {
          "scid": "sn37.10:1.5",
          "pli": "Dasamaṃ.",
          "en": ""
        }
      ],
