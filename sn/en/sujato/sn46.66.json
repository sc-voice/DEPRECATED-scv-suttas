        {
          "scid": "sn46.66:1.1",
          "pli": "“Ānāpānassati, bhikkhave, bhāvitā … pe …",
          "en": "“Mendicants, when mindfulness of breathing is developed and cultivated it’s very fruitful and beneficial. …”"
        },
        {
          "scid": "sn46.66:1.2",
          "pli": "dasamaṃ.",
          "en": ""
        },
        {
          "scid": "sn46.66:1.3",
          "pli": "Ānāpānavaggo sattamo.",
          "en": ""
        },
        {
          "scid": "sn46.66:2.1",
          "pli": "Aṭṭhikapuḷavakaṃ vinīlakaṃ,",
          "en": ""
        },
        {
          "scid": "sn46.66:2.2",
          "pli": "Vicchiddakaṃ uddhumātena pañcamaṃ;",
          "en": ""
        },
        {
          "scid": "sn46.66:2.3",
          "pli": "Mettā karuṇā muditā,",
          "en": ""
        },
        {
          "scid": "sn46.66:2.4",
          "pli": "Upekkhā ānāpānena te dasāti.",
          "en": ""
        }
      ],
