    {
      "segments": [
        {
          "scid": "sn34.16:1.1",
          "pli": "Sāvatthinidānaṃ.",
          "en": "At Sāvatthī."
        },
        {
          "scid": "sn34.16:1.2",
          "pli": "“Cattārome, bhikkhave, jhāyī.",
          "en": "“Mendicants, there are these four meditators."
        },
        {
          "scid": "sn34.16:1.3",
          "pli": "Katame cattāro?",
          "en": "What four?"
        },
        {
          "scid": "sn34.16:1.4",
          "pli": "Idha, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo hoti, na samādhismiṃ abhinīhārakusalo.",
          "en": "One meditator is skilled in entering immersion but not in projecting the mind purified by immersion. …”"
        },
        {
          "scid": "sn34.16:1.5",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ abhinīhārakusalo hoti, na samādhismiṃ samāpattikusalo.",
          "en": ""
        },
        {
          "scid": "sn34.16:1.6",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī neva samādhismiṃ samāpattikusalo hoti, na ca samādhismiṃ abhinīhārakusalo.",
          "en": ""
        },
        {
          "scid": "sn34.16:1.7",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo ca hoti, samādhismiṃ abhinīhārakusalo ca.",
          "en": ""
        },
        {
          "scid": "sn34.16:1.8",
          "pli": "Tatra … pe …",
          "en": ""
        },
        {
          "scid": "sn34.16:1.9",
          "pli": "pavaro cā”ti.",
          "en": ""
        },
        {
          "scid": "sn34.16:1.10",
          "pli": "Soḷasamaṃ.",
          "en": ""
        }
      ],
