        {
          "scid": "sn45.92-95:1.1",
          "pli": "“Seyyathāpi, bhikkhave, yamunā nadī pācīnaninnā pācīnapoṇā pācīnapabbhārā;",
          "en": "“Mendicants, the Yamunā river slants, slopes, and inclines to the east. …”"
        },
        {
          "scid": "sn45.92-95:1.2",
          "pli": "evameva kho, bhikkhave … pe …",
          "en": ""
        },
        {
          "scid": "sn45.92-95:1.3",
          "pli": "seyyathāpi, bhikkhave, aciravatī nadī pācīnaninnā pācīnapoṇā pācīnapabbhārā;",
          "en": "“… the Aciravatī river …”"
        },
        {
          "scid": "sn45.92-95:1.4",
          "pli": "evameva kho, bhikkhave … pe …",
          "en": ""
        },
        {
          "scid": "sn45.92-95:1.5",
          "pli": "seyyathāpi, bhikkhave, sarabhū nadī pācīnaninnā pācīnapoṇā pācīnapabbhārā;",
          "en": "“… the Sarabhū river …”"
        },
        {
          "scid": "sn45.92-95:1.6",
          "pli": "evameva kho, bhikkhave … pe …",
          "en": ""
        },
        {
          "scid": "sn45.92-95:1.7",
          "pli": "seyyathāpi, bhikkhave, mahī nadī pācīnaninnā pācīnapoṇā pācīnapabbhārā; evameva kho, bhikkhave … pe ….",
          "en": "“… the Mahī river …”"
        },
        {
          "scid": "sn45.92-95:1.8",
          "pli": "Pañcamaṃ.",
          "en": ""
        }
      ],
