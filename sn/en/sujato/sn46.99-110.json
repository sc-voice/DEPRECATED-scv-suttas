    {
      "segments": [
        {
          "scid": "sn46.99-110:1.1",
          "pli": "“Seyyathāpi, bhikkhave, ye keci balakaraṇīyā kammantā karīyan”ti vitthāretabbaṃ.",
          "en": "“Mendicants, all the hard work that gets done depends on the earth and is grounded on the earth. …”"
        },
        {
          "scid": "sn46.99-110:1.2",
          "pli": "Balakaraṇīyavaggo ekādasamo.",
          "en": ""
        },
        {
          "scid": "sn46.99-110:2.1",
          "pli": "Balaṃ bījañca nāgo ca,",
          "en": ""
        },
        {
          "scid": "sn46.99-110:2.2",
          "pli": "rukkho kumbhena sūkiyā;",
          "en": ""
        },
        {
          "scid": "sn46.99-110:2.3",
          "pli": "Ākāsena ca dve meghā,",
          "en": ""
        },
        {
          "scid": "sn46.99-110:2.4",
          "pli": "nāvā āgantukā nadīti.",
          "en": ""
        },
        {
          "scid": "sn46.99-110:3.1",
          "pli": "(Balakaraṇīyavaggo bojjhaṅgasaṃyuttassa bojjhaṅgavasena vitthāretabbo.)",
          "en": "(To be expanded as in SN 45.149–160.)"
        }
      ],
