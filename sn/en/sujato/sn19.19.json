        {
          "scid": "sn19.19:1.1",
          "pli": "“Addasaṃ sikkhamānaṃ vehāsaṃ gacchantiṃ.",
          "en": "“I saw a female probationer flying through the air."
        },
        {
          "scid": "sn19.19:1.2",
          "pli": "Tassā saṅghāṭipi ādittā … pe …",
          "en": "Her outer robe was burning …” …"
        },
        {
          "scid": "sn19.19:1.3",
          "pli": "pāpasikkhamānā ahosi … pe ….",
          "en": "“She used to be a bad female probationer …”"
        },
        {
          "scid": "sn19.19:1.4",
          "pli": "Navamaṃ.",
          "en": ""
        }
