        {
          "scid": "an11.502-981:4.1",
          "pli": "AN 11.502–981",
          "en": ""
        },
        {
          "scid": "an11.502-981:5.1",
          "pli": "Ekādasahi, bhikkhave, aṅgehi samannāgato gopālako bhabbo gogaṇaṃ pariharituṃ phātikātuṃ.",
          "en": ""
        },
        {
          "scid": "an11.502-981:5.2",
          "pli": "Katamehi ekādasahi?",
          "en": ""
        },
        {
          "scid": "an11.502-981:5.3",
          "pli": "Idha, bhikkhave, gopālako rūpaññū hoti…",
          "en": ""
        },
        {
          "scid": "an11.502-981:5.4",
          "pli": "pe…",
          "en": ""
        },
        {
          "scid": "an11.502-981:5.5",
          "pli": ".",
          "en": "."
        },
        {
          "scid": "an11.502-981:6.1",
          "pli": "Evameva kho, bhikkhave, ekādasahi dhammehi samannāgato bhikkhu bhabbo cakkhusmiṃ aniccānupassī viharituṃ…",
          "en": ""
        },
        {
          "scid": "an11.502-981:6.2",
          "pli": "pe…",
          "en": ""
        },
        {
          "scid": "an11.502-981:6.3",
          "pli": "paṭinissaggānupassī viharitunti.",
          "en": ""
        }
      ],
