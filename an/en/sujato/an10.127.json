    },
    {
      "segments": [
        {
          "scid": "an10.127:1.1",
          "pli": "“Dasayime, bhikkhave, dhammā ekantanibbidāya virāgāya nirodhāya upasamāya abhiññāya sambodhāya nibbānāya saṃvattanti, nāññatra sugatavinayā.",
          "en": "“Mendicants, these ten things don’t lead solely to disillusionment, fading away, cessation, peace, insight, awakening, and extinguishment apart from the Holy One’s training."
        },
        {
          "scid": "an10.127:1.2",
          "pli": "Katame dasa?",
          "en": "What ten?"
        },
        {
          "scid": "an10.127:1.3",
          "pli": "Sammādiṭṭhi … pe … sammāvimutti—",
          "en": "Right view, right thought, right speech, right action, right livelihood, right effort, right mindfulness, right immersion, right knowledge, and right freedom."
        },
        {
          "scid": "an10.127:1.4",
          "pli": "ime kho, bhikkhave, dasa dhammā ekantanibbidāya virāgāya nirodhāya upasamāya abhiññāya sambodhāya nibbānāya saṃvattanti, nāññatra sugatavinayā”ti.",
          "en": "These are the ten things that don’t lead solely to disillusionment, fading away, cessation, peace, insight, awakening, and extinguishment apart from the Holy One’s training.”"
        },
        {
          "scid": "an10.127:1.5",
          "pli": "Pañcamaṃ.",
          "en": ""
        }
      ],
