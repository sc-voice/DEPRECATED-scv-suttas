        {
          "scid": "an3.148:1.1",
          "pli": "“Tīhi, bhikkhave … pe …",
          "en": "“Someone with three qualities is cast down to hell. …"
        },
        {
          "scid": "an3.148:1.2",
          "pli": "visamena kāyakammena, visamena vacīkammena, visamena manokammena—",
          "en": "Unethical deeds by way of body, speech, and mind. …"
        },
        {
          "scid": "an3.148:1.3",
          "pli": "imehi kho … pe … evaṃ niraye.",
          "en": ""
        },
        {
          "scid": "an3.148:2.1",
          "pli": "Tīhi, bhikkhave, dhammehi … pe …",
          "en": "Someone with three qualities is raised up to heaven. …"
        },
        {
          "scid": "an3.148:2.2",
          "pli": "samena kāyakammena, samena vacīkammena, samena manokammena—",
          "en": "Ethical deeds by way of body, speech, and mind. …”"
        },
        {
          "scid": "an3.148:2.3",
          "pli": "imehi kho … pe … evaṃ sagge”ti.",
          "en": ""
        },
        {
          "scid": "an3.148:2.4",
          "pli": "Tatiyaṃ.",
          "en": ""
        }
      ],
